#%Module 1.0
#
#  MVAPICH module for use with 'environment-modules' package:
#
conflict                mpi
conflict                mpich
prepend-path            PATH            /global/kgt/mvapich/bin
prepend-path            LD_LIBRARY_PATH /global/kgt/mvapich/lib
prepend-path            MANPATH         /global/kgt/mvapich/man
prepend-path            PKG_CONFIG_PATH /global/kgt/mvapich/lib/pkgconfig
setenv                  MPI_BIN         /global/kgt/mvapich/bin
setenv                  MPI_INCLUDE     /global/kgt/mvapich/include
setenv                  MPI_LIB         /global/kgt/mvapich/lib
setenv                  MPI_MAN         /global/kgt/mvapich/man
setenv                  MPI_HOME        /global/kgt/mvapich
